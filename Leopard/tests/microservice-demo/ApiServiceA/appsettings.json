{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ServiceDiscovery": {
    "ServiceName": "ApiServiceA",
    "Consul": {
      "HttpEndpoint": "http://127.0.0.1:8500"
    },
    "HealthCheck": {
      "HealthCheckTemplate": "/api/health",
      "Interval": 10
    }
  }
}
